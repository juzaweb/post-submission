{
    "name": "juzaweb/post-submission",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Plugin support user can submit post to your website",
    "authors": [
        {
            "name": "Juzaweb Team",
            "email": "support@juzaweb.com",
            "homepage": "https://juzaweb.com"
        }
    ],
    "extra": {
        "juzaweb": {
            "name": "Post Submition",
            "domain": "post_submit",
            "providers": [
                "Juzaweb\\PostSubmission\\Providers\\PostSubmitServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\PostSubmission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\PostSubmission\\Tests\\": "tests/"
        }
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
